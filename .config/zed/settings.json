{
  "auto_update": false,
  "buffer_font_family": "JetBrains Mono",
  "buffer_font_size": 14,
  "buffer_font_weight": 200,
  "vim_mode": true,
  "drop_target_size": 0.33,
  "features": {},
  "gutter": {
    "folds": false
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "message_editor": {
    "auto_replace_emoji_shortcode": true
  },
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "check": {
          "command": "clippy" // rust-analyzer.check.command (default: "check")
        }
      }
    }
  },
  "preview_tabs": {
    "enable_preview_from_code_navigation": true,
    "enable_preview_from_file_finder": true
  },
  "project_panel": {
    "auto_fold_dirs": true
  },
  "tabs": {
    "git_status": true
  },
  "telemetry": {
    "enabled": false,
    "metrics": false,
    "diagnostics": false
  },
  "terminal": {
    "font_weight": 200,
    "font_family": "JetBrains Mono",
    "font_size": 14,
    "shell": {
      "program": "/usr/bin/zsh"
    },
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Phine"
  },
  "ui_font_size": 16,
  "ui_font_weight": 200,
  "unnecessary_code_fade": 0.8,
  "unstable.ui_density": "default",
  "use_smartcase_search": true,
  "wrap_guides": [80, 120]
}
